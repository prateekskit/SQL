    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Grp">
            <Value>=Parameters!Grp.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select DST.SalesTerritoryGroup,DST.SalesTerritoryCountry,DST.SalesTerritoryRegion,
			DST.SalesTerritoryKey,
			 FIS.SalesAmount
		,FIS.TotalProductCost
		 from FactInternetSales FIS inner join DimSalesTerritory DST
	on FIS.SalesTerritoryKey = DST.SalesTerritoryKey
WHERE DST.SalesTerritoryGroup = @Grp</CommandText>
      </Query>
      <Fields>
        <Field Name="SalesTerritoryGroup">
          <DataField>SalesTerritoryGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
